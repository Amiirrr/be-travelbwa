{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "server.js",
            "methods": "*",
            "headers": {
                "Access-Control-Allow-Origin": "http://localhost:5173, http://localhost:3000, https://be-travelbwa.vercel.app",
                "Access-Control-Allow-Credentials": "true",
                "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
            }
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 5 * * *"
        }
    ]
}